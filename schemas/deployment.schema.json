{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Deployment",
  "description": "The JSON object of an OGC STA-2.0 `Deployment` entity.",
  "type": "object",
  "properties": {
    "@context": {
      "type": "object",
      "properties": {
        "@version": {
          "type": "number",
          "const": 1.1
        },
        "@import": {
          "type": "string"
        },
        "@vocab": {
          "type": "string",
          "const": "http://schema.org/"
        }
      },
      "required": [
        "@version",
        "@import",
        "@vocab"
      ]
    },
    "@type": {
      "type": "string",
      "const": "Deployment"
    },
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "atDepth": {
      "type": "number"
    },
    "depthUom": {
      "type": "string"
    },
    "deploymentTime": {
      "type": "string"
    },
    "properties": {
      "type": "object",
      "properties": {
        "@id": {
          "$ref": "#/$defs/url"
        },
        "offsets": {
          "type": "object",
          "properties": {
            "x": {
              "type": "number"
            },
            "y": {
              "type": "number"
            },
            "z": {
              "type": "number"
            }
          }
        },
        "offsetsUom": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "@context",
    "@type",
    "name"
  ],
  "$defs": {
    "url": {
      "type": "string",
      "pattern": "^https*://"
    },
    "organization": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string",
          "const": "Organization"
        },
        "name": {
          "type": "string"
        },
        "identifier": {
          "type": "string",
          "pattern": "^https*://"
        }
      },
      "required": [
        "@type",
        "name"
      ]
    }
  }
}
