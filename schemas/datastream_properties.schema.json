{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "DatastreamProperty",
  "description": "The JSON object of the `properties` property of an OGC STA `DataStream` entity.",
  "type": "object",
  "properties": {
    "@context": {
      "type": "object",
      "properties": {
        "@version": {
          "type": "number",
          "const": 1.1
        },
        "@import": {
          "type": "string"
        },
        "@base": {
          "type": "string",
          "const": "https://codebase.helmholtz.cloud/stamplate#"
        },
        "@vocab": {
          "type": "string",
          "const": "http://schema.org/"
        }
      }
    },
    "@id": {
      "type": "string",
      "pattern": "^https*://"
    },
    "@type": {
      "type": "string",
      "const": "DatastreamProperties"
    },
    "GeoCoordinates": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        },
        "elevation": {
          "type": "string"
        }
      }
    },
    "license": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "pattern": "^https*://"
        }
      }
    }
  }
}
