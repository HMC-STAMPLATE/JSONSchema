{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "DatastreamProperty",
  "description": "The JSON object of the `properties` property of an OGC STA `DataStream` entity.",
  "type": "object",
  "properties": {
    "@context": {
      "type": "object",
      "properties": {
        "@version": {
          "type": "number",
          "const": 1.1
        },
        "@import": {
          "type": "string"
        },
        "@vocab": {
          "type": "string",
          "const": "http://schema.org/"
        }
      },
      "required": ["@version", "@import", "@vocab"]
    },
    "@id": {
      "type": "string",
      "pattern": "^https*://"
    },
    "@type": {
      "type": "string",
      "const": "DatastreamProperties"
    },
    "providerMobility": {
      "type": "string",
      "enum": ["static", "dynamic", "profiling"]
    },
    "geoCoordinates": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string",
          "const": "GeoCoordinates"
        },
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        },
        "elevation": {
          "type": "string"
        }
      },
      "required": ["@type", "latitude", "longitude", "elevation"]
    },
    "license": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string",
          "const": "CreativeWork"
        },
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "pattern": "^https*://"
        }
      },
      "required": ["@type", "name"]
    }
  },
  "required": ["@context", "@type", "@id"]
}
